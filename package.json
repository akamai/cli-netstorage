{
  "name": "akamaiclinetstorage",
  "version": "1.0.2",
  "description": "A CLI to interface with Akamai's NetStorage platform.",
  "repository": "https://github.com/akamai/cli-netstorage",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "start": "node bin/akamaiNetStorage",
    "pretest": "npm run lint",
    "test": "mocha test --timeout 0",
    "build": "pkg bin/akamaiProperty --target node8-linux-x86,node8-linux-x64,node8-win-x86,node8-win-x64,node8-macos-x64 --output akamai-netstorage-1.0.0"
  },
  "main": "./index.js",
  "files": [
    "index.js",
    "src"
  ],
  "dependencies": {
    "chalk": "^2.3.0",
    "child-process-promise": "^2.2.1",
    "ini": "^1.3.5",
    "inquirer": "^4.0.2",
    "log4js": "^2.9.0",
    "md5": "^2.2.1",
    "merge": "^1.2.0",
    "mocha": "^3.2.0",
    "moment": "^2.20.1",
    "mz": "^2.7.0",
    "options": "0.0.6",
    "pkg": "^4.3.2",
    "request": "^2.87.0",
    "safe-buffer": "^5.1.1",
    "sywac": "^1.2.0",
    "untildify": "*"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-loopback": "^10.0.0"
  }
}
